# Environment Variables для Production деплоя на Render

# Database
DATABASE_URL="postgresql://username:password@hostname:port/database"

# JWT Secret (используйте сложный пароль в production)
JWT_SECRET="your-super-secret-jwt-key-change-in-production"

# Environment
NODE_ENV="production"

# Port (Render автоматически установит)
PORT=3001

# CORS Origins (добавьте ваши домены)
CORS_ORIGINS="https://socialbot-frontend.onrender.com,http://localhost:3000" 